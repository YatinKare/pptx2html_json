{
  "name": "@sylphlab/tools-adaptor-mcp",
  "version": "0.3.1",
  "description": "Adapter to convert internal SylphTool definitions to MCP SDK format.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "mcp",
    "adapter",
    "tool"
  ],
  "author": "Sylph Lab",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.2",
    "zod": "^3.23.8",
    "@sylphlab/tools-core": "0.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "tsup": "^8.4.0",
    "typescript": "^5.6.2",
    "vitest": "^3.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "lint": "biome lint"
  }
}